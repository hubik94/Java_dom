package lekcja32;

class Machine{
	public void start(){
		System.out.println("Machine started");
	}
}

class Camera extends Machine{
	public void start(){
		System.out.println("Camera started");
	}
	
	public void snap(){
		System.out.println("Photo taken");
	}
}

public class Glowna {
	public static void main(String[] args) {
		Machine maszyna1 = new Machine();
	}
}
