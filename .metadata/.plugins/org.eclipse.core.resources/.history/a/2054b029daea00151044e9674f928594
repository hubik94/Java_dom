package lekcja32;

public class Glowna {
	public static void main(String[] args) {
		byte byteValue = 20;
		short shortValue = 45;
		int wartosc = 888;
		long longValue = 52226;
		
		float floatValue = 8834.6f;
		double doubleValue = 32.4;
		
		System.out.println(Integer.MAX_VALUE);
		
		wartosc = (int)longValue;
		
		System.out.println(wartosc);
		
		doubleValue = wartosc;
		System.out.println(doubleValue);
		
		wartosc = (int)floatValue;
		System.out.println(wartosc);
		
		byteValue = (byte)128;
		System.out.println(byteValue);
	}
}
